#include "./boards/shields/klink_kbd/common.keymap"
#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&gresc            &kp Q         &kp W         &kp E  &kp R        &kp T  &kp Y      &kp U  &kp I   &kp O       &kp P            &trans   &kp BSPC
&kp LEFT_CONTROL  &kp A         &kp S         &kp D  &kp F        &kp G  &kp H      &kp J  &kp K   &kp L       &trans           &kp RET
&kp LSHFT         &kp Z         &kp X         &kp C  &kp V        &kp B  &kp N      &kp M  &trans  &kp PERIOD  &kp RIGHT_SHIFT  &mo 1
                  &kp LEFT_ALT  &kp LEFT_WIN         &lt 1 SPACE         &kp SPACE                 &tog 3      &mo 2
            >;
        };

        number {
            bindings = <
&trans        &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3  &kp NUMBER_4  &kp NUMBER_5  &kp NUMBER_6  &kp N7  &kp N8  &kp N9    &kp UP_ARROW  &kp N0  &trans
&kp CAPSLOCK  &trans        &trans        &trans        &trans        &trans        &trans        &trans  &trans  &kp LEFT  &kp RIGHT     &trans
&trans        &trans        &trans        &trans        &trans        &trans        &trans        &trans  &trans  &kp DOWN  &trans        &trans
              &trans        &trans                      &trans                      &trans                        &trans    &trans
            >;
        };

        specialA {
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans     &trans      &trans         &kp LEFT_BRACKET  &kp RIGHT_BRACKET  &kp DELETE
&trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans     &trans      &kp SEMICOLON  &kp SQT           &trans
